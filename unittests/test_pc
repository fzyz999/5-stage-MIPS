#! /usr/bin/vvp
:ivl_version "0.9.6 " "(v0_9_6)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x101ed60 .scope module, "test_pc" "test_pc" 2 1;
 .timescale 0 0;
v0x10532d0_0 .var "clk", 0 0;
v0x10533a0_0 .net "instr", 31 2, L_0x10535e0; 1 drivers
v0x1053450_0 .var "npc", 31 2;
v0x1053500_0 .var "reset", 0 0;
E_0x101e0e0 .event negedge, v0x101c1e0_0;
S_0x1041c80 .scope module, "_pc" "pc" 2 6, 3 1, S_0x101ed60;
 .timescale 0 0;
v0x101c1e0_0 .net "clk", 0 0, v0x10532d0_0; 1 drivers
v0x1053040_0 .var "internal_pc", 31 0;
v0x10530e0_0 .net "npc", 31 2, v0x1053450_0; 1 drivers
v0x1053180_0 .alias "pc", 31 2, v0x10533a0_0;
v0x1053230_0 .net "reset", 0 0, v0x1053500_0; 1 drivers
E_0x101c370 .event posedge, v0x101c1e0_0;
E_0x1042200 .event edge, v0x1053230_0;
L_0x10535e0 .part v0x1053040_0, 0, 30;
    .scope S_0x1041c80;
T_0 ;
    %wait E_0x1042200;
    %load/v 8, v0x1053230_0, 1;
    %jmp/0xz  T_0.0, 8;
    %movi 8, 12288, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0x1053040_0, 0, 8;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1041c80;
T_1 ;
    %wait E_0x101c370;
    %load/v 8, v0x10530e0_0, 30;
    %mov 38, 0, 2;
    %ix/load 0, 32, 0;
    %assign/v0 v0x1053040_0, 0, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_0x101ed60;
T_2 ;
    %set/v v0x10532d0_0, 0, 1;
    %set/v v0x1053450_0, 0, 30;
    %set/v v0x1053500_0, 1, 1;
    %delay 7, 0;
    %set/v v0x1053500_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x101ed60;
T_3 ;
    %delay 5, 0;
    %load/v 8, v0x10532d0_0, 1;
    %inv 8, 1;
    %set/v v0x10532d0_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x101ed60;
T_4 ;
    %delay 30, 0;
    %load/v 8, v0x1053500_0, 1;
    %inv 8, 1;
    %set/v v0x1053500_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x101ed60;
T_5 ;
    %wait E_0x101e0e0;
    %load/v 8, v0x1053450_0, 30;
    %mov 38, 0, 2;
    %addi 8, 4, 32;
    %ix/load 0, 30, 0;
    %assign/v0 v0x1053450_0, 0, 8;
    %jmp T_5;
    .thread T_5;
    .scope S_0x101ed60;
T_6 ;
    %delay 200, 0;
    %vpi_call 2 23 "$finish";
    %end;
    .thread T_6;
    .scope S_0x101ed60;
T_7 ;
    %vpi_call 2 27 "$monitor", "time %t reset %d npc %x instr %x", $time, v0x1053500_0, v0x1053450_0, v0x10533a0_0;
    %end;
    .thread T_7;
    .scope S_0x101ed60;
T_8 ;
    %vpi_call 2 31 "$dumpfile", "test_pc.lxt";
    %vpi_call 2 32 "$dumpvars", 1'sb0, S_0x101ed60;
    %end;
    .thread T_8;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_pc.v";
    "../datapath/pc.v";
